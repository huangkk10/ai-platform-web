{
  "name": "ai-platform-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "AI Platform Frontend using Ant Design of React as primary UI framework",
  "uiFramework": {
    "primary": "antd",
    "version": "^5.27.3",
    "philosophy": "Ant Design First - All UI components should prioritize Ant Design (antd) over other libraries"
  },
  "markdownSystem": {
    "primary": "react-markdown",
    "version": "^10.1.0",
    "plugins": [
      "remark-gfm"
    ],
    "fallback": "markdown-it (for editors only)",
    "note": "Migrated from markdown-it to react-markdown for better React integration and table+image rendering"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.27.3",
    "axios": "^1.4.0",
    "dayjs": "^1.11.18",
    "dompurify": "^3.2.7",
    "markdown-it": "^14.1.0",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-markdown-editor-lite": "^1.3.4",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.2.1",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
